# Focus specific windows
cmd + ctrl - 6: open -a Slack
cmd + ctrl - 7: open -a Firefox
cmd + ctrl - 8: open -a IntelliJ\ IDEA
cmd + ctrl - 9: open -a Terminal
cmd + ctrl - 0: open -a Google\ Chrome

# Change window focus
cmd + ctrl - p : yabai -m window --focus north || yabai -m display --focus north || yabai -m space --focus next
cmd + ctrl - f : yabai -m window --focus east || yabai -m display --focus east
cmd + ctrl - n : yabai -m window --focus south || yabai -m display --focus south || yabai -m space --focus prev
cmd + ctrl - b : yabai -m window --focus west || yabai -m display --focus west

# Move window
cmd + ctrl + shift - p : yabai -m window --swap north
cmd + ctrl + shift - f : yabai -m window --swap east
cmd + ctrl + shift - n : yabai -m window --swap south
cmd + ctrl + shift - b : yabai -m window --swap west
cmd + ctrl + shift - up : yabai -m window --swap north
cmd + ctrl + shift - right : yabai -m window --swap east
cmd + ctrl + shift - down : yabai -m window --swap south
cmd + ctrl + shift - left : yabai -m window --swap west

# Move window to another space
cmd + ctrl + shift - 1 : yabai -m window --space 1
cmd + ctrl + shift - 2 : yabai -m window --space 2
cmd + ctrl + shift - 3 : yabai -m window --space 3
cmd + ctrl + shift - 4 : yabai -m window --space 4
cmd + ctrl + shift - 5 : yabai -m window --space 5
cmd + ctrl + shift - 6 : yabai -m window --space 6
cmd + ctrl + shift - 7 : yabai -m window --space 7
cmd + ctrl + shift - 8 : yabai -m window --space 8
cmd + ctrl + shift - 9 : yabai -m window --space 9

# Resize window
cmd + ctrl - 0x91 : yabai -m window --resize rel:0:10
cmd + ctrl - 0x88 : yabai -m window --resize rel:10:0
cmd + ctrl - 0x84 : yabai -m window --resize rel:0:-10
cmd + ctrl - 0x86 : yabai -m window --resize rel:-10:0

# Change space focus
cmd + ctrl - 1 : yabai -m space --focus 1
cmd + ctrl - 2 : yabai -m space --focus 2
cmd + ctrl - 3 : yabai -m space --focus 3
cmd + ctrl - 4 : yabai -m space --focus 4
cmd + ctrl - 5 : yabai -m space --focus 5
cmd + ctrl - 6 : yabai -m space --focus 6
cmd + ctrl - 7 : yabai -m space --focus 7
cmd + ctrl - 8 : yabai -m space --focus 8
cmd + ctrl - 9 : yabai -m space --focus 9
